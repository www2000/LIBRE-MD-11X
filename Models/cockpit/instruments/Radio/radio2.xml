<PropertyList>

    <path>radio.ac</path>

    <animation>
        <type>select</type>
        <object-name>Radio.pwr</object-name>
        <object-name>freq</object-name>
        <object-name>freq.001</object-name>
        <object-name>freq.002</object-name>
        <object-name>freq.003</object-name>
        <object-name>freq.004</object-name>
        <object-name>freq.005</object-name>
        <object-name>freq.006</object-name>
        <object-name>freq.007</object-name> 
        <object-name>stby</object-name>
        <object-name>stby.001</object-name>
        <object-name>stby.002</object-name>
        <object-name>stby.003</object-name>
        <object-name>stby.004</object-name>
        <object-name>stby.005</object-name>
        <object-name>stby.006</object-name>
        <condition>
            <greater-than>
                <property>systems/electrical/CPT-FLT-INST</property>
                <value>5</value>
            </greater-than>
        </condition>
    </animation>


<!-- Active freq nav mode -->

    <animation>
        <type>textranslate</type>
        <object-name>freq.007</object-name>
        <property>instrumentation/efis/radio-mode</property>
        <factor>0.1520</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.001</object-name>
        <property>instrumentation/efis/radio-selected</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.002</object-name>
        <property>instrumentation/efis/radio-selected</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.003</object-name>
        <property>instrumentation/efis/radio-selected</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.004</object-name>
        <property>instrumentation/efis/radio-selected</property>
        <factor>1.0</factor>
        <step>0.1</step>
        <bias>0.0005</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.005</object-name>
        <property>instrumentation/efis/radio-selected</property>
        <factor>10.0</factor>
        <step>0.01</step>
        <bias>0.0005</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.006</object-name>
        <property>instrumentation/efis/radio-selected</property>
        <factor>100.0</factor>
        <step>0.001</step>
        <bias>0.0005</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!-- Standby freq-->
    <animation>
        <type>textranslate</type>
        <object-name>stby.001</object-name>
        <property>instrumentation/efis/radio-standby</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>stby.002</object-name>
        <property>instrumentation/efis/radio-standby</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>stby.003</object-name>
        <property>instrumentation/efis/radio-standby</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>stby.004</object-name>
        <property>instrumentation/efis/radio-standby</property>
        <factor>1.0</factor>
        <step>0.1</step>
        <bias>0.0005</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>stby.005</object-name>
        <property>instrumentation/efis/radio-standby</property>
        <factor>10.0</factor>
        <step>0.01</step>
        <bias>0.0005</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>stby.006</object-name>
        <property>instrumentation/efis/radio-standby</property>
        <factor>100.0</factor>
        <step>0.001</step>
        <bias>0.0005</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!-- Hotspots -->
    <animation>
        <type>pick</type>
        <object-name>swp.btn</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
             <binding>
                <command>nasal</command>
                <script>b777.Efis.swap_freq()</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>vhf-l.btn</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
             <binding>
                <command>nasal</command>
                <script>b777.Efis.set_radio_mode(0)</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>vhf-r.btn</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
             <binding>
                <command>nasal</command>
                <script>b777.Efis.set_radio_mode(1)</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>hf-l.btn</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
             <binding>
                <command>nasal</command>
                <script>b777.Efis.set_radio_mode(2)</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>hf-r.btn</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
             <binding>
                <command>nasal</command>
                <script>b777.Efis.set_radio_mode(3)</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>stby.up</object-name>
        <visible>false</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(1)</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>stby.up</object-name>
        <visible>false</visible>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(-1)</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>stby.dn</object-name>
        <visible>false</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(-1)</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>stby.dn</object-name>
        <visible>false</visible>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(1)</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>stby.up.001</object-name>
        <visible>false</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(10)</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>stby.up.001</object-name>
        <visible>false</visible>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(-10)</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>stby.dn.001</object-name>
        <visible>false</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(-10)</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>stby.dn.001</object-name>
        <visible>false</visible>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
             <binding>
             <command>nasal</command>
            <script>b777.Efis.set_freq(10)</script>
            </binding>
        </action>
    </animation>

</PropertyList>
