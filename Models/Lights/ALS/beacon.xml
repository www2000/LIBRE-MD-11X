<PropertyList>

 <path>beacon.ac</path>
 <nopreview/>


 <animation>
  <type>scale</type>
  <x-offset>1</x-offset>
  <y-offset>1</y-offset>
  <z-offset>1</z-offset>
 </animation>
 

 
<animation>
  <type>dist-scale</type>
  <object-name>beaconals</object-name>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>1</dep>
   </entry>
   <entry>
    <ind>300</ind>
    <dep>4</dep>
   </entry>
   <entry>
    <ind>1500</ind>
    <dep>8</dep>
   </entry>
  </interpolation>
 </animation>
 

 
<effect>
   <inherits-from>Aircraft/LIBRE-MD-11/Models/lights/ALS/beacon</inherits-from>
   <object-name>beaconals</object-name>
 </effect>


</PropertyList>
